import json


file1 = "/home/maz2h/Arxiv-papers2000/sample_nonnull_cleaned_10000.json_with_llama3.3-70B_sent_p1.json"
file2 = "/home/maz2h/Arxiv-papers2000/sample_nonnull_cleaned_10000.json_with_llama3.3-70B_sent_p2.json"

# define output file path
output_file = "/home/maz2h/Arxiv-papers2000/sample_nonnull_cleaned_10000.json_with_llama3.3-70B_sent_llama3.json"

# Load data from both files
with open(file1, "r") as f1, open(file2, "r") as f2:
    data1 = json.load(f1)
    data2 = json.load(f2)

# Merge the data which are in two files
merged_data = data1 + data2

# Save the merged data
with open(output_file, "w") as fout:
    json.dump(merged_data, fout, indent=2)

print(f"Merged file saved as {output_file}")
